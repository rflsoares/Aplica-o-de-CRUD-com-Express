<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% docs.forEach(function(customer){ %>
        <li>
            <%= customer.name %> <%= customer.sobrenome %> - <b>Age: </b><%= customer.age %> years old.
        </li>
      <% }) %>
    </ul>
  </body>
</html>
